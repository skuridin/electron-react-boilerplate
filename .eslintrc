{
  "env": {
    "node": true,
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "new-cap": 0,
    "object-curly-spacing": [2, "always"],
    "space-in-parens": [2, "never"],
    "arrow-spacing": 2,
    "object-shorthand": 2,
    "strict": [2, "global"],
    "no-console": 0
  },
  "extends": "airbnb/base"
}
